{
  "WebServiceReferences": {
    "EmailService": "http://dev.edugamecloud.com/services/EmailService.svc",
    "CompanyEventsService": "http://dev.edugamecloud.com/services/CompanyEventsService.svc",
    "CompanyAcDomainsService": "http://dev.edugamecloud.com/services/CompanyAcDomainsService.svc",
    "LookupService": "http://dev.edugamecloud.com/services/LookupService.svc",
    "QuizService": "http://dev.edugamecloud.com/services/QuizService.svc",
    "QuizResultService": "http://dev.edugamecloud.com/services/QuizResultService.svc",
    "FileService": "http://dev.edugamecloud.com/services/FileService.svc"
  },
  "CertificateSettings": {
    "PdfOutputFolder": "~/App_Data/Pdf/Shared/",
    "PdfTemplateFolder": "~/App_Data/Pdf/Template/"
  },

  "Logging": {
    "IncludeScopes": false,
    "PathFormat": ".\\Logs\\\\DEBUG-{Date}.txt",
    "LogLevel": {
      "Default": "Debug"
    }
  },

  "Logging_Serilog_Errors": {
    "IncludeScopes": true,
    "PathFormat": ".\\Logs\\\\ERR-{Date}.txt",
    "LogLevel": {
      "Default": "Error",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    }
  }
}