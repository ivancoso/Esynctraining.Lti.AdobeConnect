{ 
  "WebServiceReferences": {
    "EmailService": "http://app.edugamecloud.com/services/EmailService.svc",
    "CompanyEventsService": "http://app.edugamecloud.com/services/CompanyEventsService.svc",
    "CompanyAcDomainsService": "http://app.edugamecloud.com/services/CompanyAcDomainsService.svc",
    "LookupService": "http://app.edugamecloud.com/services/LookupService.svc"
  },
  "PdfTemplateFolder": "~/App_Data/Pdf/Template/",
  "PdfOutputFolder\"": "~/App_Data/Pdf/Shared/",

  "Logging": {
    "IncludeScopes": false,
    "PathFormat": ".\\Logs\\\\DEBUG-{Date}.txt",
    "LogLevel": {
      "Default": "Debug"
    }
  },

  "Logging_Serilog_Errors": {
    "IncludeScopes": true,
    "PathFormat": ".\\Logs\\\\ERR-{Date}.txt",
    "LogLevel": {
      "Default": "Error",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    }
  }
}