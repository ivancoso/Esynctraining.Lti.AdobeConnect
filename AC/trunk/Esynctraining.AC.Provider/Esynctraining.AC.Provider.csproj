<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Provides Adobe Connect functionality for eSyncTraining projects</Description>
    <Copyright>Copyright � 2013-2019 eSyncTraining.com</Copyright>
    <AssemblyTitle>Esynctraining.AC.Provider</AssemblyTitle>
    <Authors>eSyncTraining.com</Authors>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <AssemblyName>Esynctraining.AC.Provider</AssemblyName>
    <PackageId>Esynctraining.AC.Provider</PackageId>
    <PackageTags>core;AC;AdobeConnect</PackageTags>
    <PackageReleaseNotes>Use Polly to retry if login response cookie is invalid. netstandard2.0 ONLY.</PackageReleaseNotes>
    <Version>1.0.6.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
    <AssemblyVersion>1.0.6.0</AssemblyVersion>
    <FileVersion>1.0.6.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NETStandard.Library" Version="2.0.3" />
    <PackageReference Include="Polly" Version="7.1.1" />
  </ItemGroup>

</Project>
