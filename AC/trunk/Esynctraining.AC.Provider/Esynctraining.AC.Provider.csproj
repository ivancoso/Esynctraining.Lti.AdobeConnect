<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Provides Adobe Connect functionality for eSyncTraining projects</Description>
    <Copyright>Copyright � 2013-2017 eSyncTraining.com</Copyright>
    <AssemblyTitle>Esynctraining.AC.Provider</AssemblyTitle>
    <Authors>eSyncTraining.com</Authors>
    <TargetFrameworks>net45;net461;netstandard2.0</TargetFrameworks>
    <AssemblyName>Esynctraining.AC.Provider</AssemblyName>
    <PackageId>Esynctraining.AC.Provider</PackageId>
    <PackageTags>core;AC;AdobeConnect</PackageTags>
    <PackageReleaseNotes>add parse range min and max</PackageReleaseNotes>
    <Version>1.0.5.3</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net45' ">
    <Reference Include="System.Xml" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net461' ">
    <Reference Include="System.Xml" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard2.0' ">
    <PackageReference Include="NETStandard.Library" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Net.Http" Version="4.3.3" />
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>

</Project>
