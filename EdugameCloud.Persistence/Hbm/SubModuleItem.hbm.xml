<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <sql-query name="getSMIQuestionsBySMIId">
		<query-param name="subModuleItemId" type="System.Int32"/>
		exec getSMIQuestionsBySMIId @subModuleItemId=:subModuleItemId
	</sql-query>
	<sql-query name="getSMIDistractorsBySMIId">
		<query-param name="subModuleItemId" type="System.Int32"/>
		exec getSMIDistractorsBySMIId @subModuleItemId=:subModuleItemId
	</sql-query>
</hibernate-mapping>
