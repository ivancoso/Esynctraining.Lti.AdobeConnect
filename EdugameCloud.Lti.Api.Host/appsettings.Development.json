{

  "Logging": {
    "IncludeScopes": false,
    "PathFormat": ".\\Logs\\{Date}.txt",
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "Logging_Serilog_Errors": {
    "IncludeScopes": true,
    "PathFormat": ".\\Logs\\ERR-{Date}.txt",
    "LogLevel": {
      "Default": "Error",
      "Override": {
        "System": "Error",
        "Microsoft": "Error"
      }
    }
  }

}
