        <AssemblyTitle>Esynctraining.AdobeConnect</AssemblyTitle>
        <Authors>eSyncTraining.com</Authors>
        <TargetFrameworks>netstandard2.0</TargetFrameworks>
        <AssemblyName>Esynctraining.AdobeConnect</AssemblyName>
        <PackageId>Esynctraining.AdobeConnect</PackageId>
        <PackageTags>AC;AdobeConnect</PackageTags>
        <PackageReleaseNotes>+MeetingFeatureInfo; enableHtml</PackageReleaseNotes>
        <Version>1.0.7.3</Version>
